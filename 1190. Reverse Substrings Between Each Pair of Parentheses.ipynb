    "def reverseParentheses(self, s: str) -> str:\n",
    "    stack = [\"\"]\n",
    "    for c in s:\n",
    "        if c == \"(\":\n",
    "            stack.append(\"\")\n",
    "        elif c == \")\":\n",
    "            add = stack.pop()[::-1]\n",
    "            stack[-1] += add\n",
    "        else:\n",
    "            stack[-1] += c\n",
    "    return stack.pop()"
   ]
  }
