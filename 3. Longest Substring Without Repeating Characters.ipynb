    "def lengthOfLongestSubstring(s):\n",
    "    l = list()\n",
    "    for c in s:\n",
    "        l.append(c)\n",
    "        if len(set(l)) < len(l):\n",
    "            l.pop(0)\n",
    "    return len(l)"
   ]
