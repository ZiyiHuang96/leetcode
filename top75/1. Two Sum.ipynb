   "source": [
    "def twoSum(nums, target):\n",
    "    seen = {}\n",
    "    for i, num in enumerate(nums):\n",
    "        remaining = target - num\n",
    "        if remaining in seen:\n",
    "            return [seen[remaining], i]\n",
    "        seen[num] = i"
   ]
