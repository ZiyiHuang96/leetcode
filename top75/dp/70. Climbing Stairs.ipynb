    "def climbStairs(n):\n",
    "    if n < 2:\n",
    "        return 1\n",
    "    dp = {0:1, 1:1}\n",
    "    i = 2\n",
    "    while i < n + 1:\n",
    "        dp[i] = dp[i-1] + dp[i-2]\n",
    "        i += 1\n",
    "    return dp[n]"
   ]
