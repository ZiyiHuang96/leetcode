    "def longestCommonPrefi( strs:[str]):\n",
    "    res = \"\"\n",
    "    for i in zip(*strs):\n",
    "        if len(set(i)) == 1:\n",
    "            res += i[0]\n",
    "        else:\n",
    "            return res\n",
    "    return res"
   ]
